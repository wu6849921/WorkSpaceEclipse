<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf lang='zh-CN' xml:lang='zh-CN'>
	<head>
		<style>
			.borderedCellT {border: 0.1pt solid black; padding:2pt;valign:middle; letter-spacing:0px;}
			.borderedCell {border: 0.1pt solid black; padding:2pt;valign:middle; letter-spacing:0px;font-size:8pt;}
			.borderedCellLineL {padding:2pt;border-style: solid black;  letter-spacing:0px;font-size:8pt;  }
			.borderedCellLine {padding:5pt;letter-spacing:0px;border-left-width: 0.1pt;border-right-width: 0.1pt;font-size:8pt;min-height:100px;}
		</style>
		<macrolist>
			<macro id="rptfooter">
				<p align="right" font-size="8pt">
					第
					<pagenumber />
					頁，共
					<totalpages />
					頁
				</p>
			</macro>
		</macrolist>
	</head>
	<body size="A4" footer="rptfooter" footer-height="20mm"
		font-family="stsong">
		<table  style="width: 100%;">
			<tr>
				<th align="center" colspan="7">
					<span style="font-size:20px;">
						<strong>MEDAL SPORTS</strong>
					</span>
				</th>
			</tr>
			<tr>
				<th align="center" colspan="7"><br/></th>
			</tr>
			<tr>
				<th align="center" colspan="7"><u>支出申請單</u>  </th>
			</tr>
			<tr>
				<th class="borderedCellT" >
					<strong>Vendor</strong>
				</th>
				<th class="borderedCell" colspan="3">
					${renderData.vendor}
				</th>
				<th class="borderedCellT">
					<strong>Date</strong>
				</th>
				<th class="borderedCell" colspan="2">
					${renderData.date}
				</th>
			</tr>
			<tr>
				<td class="borderedCellT">
					<strong>Customer</strong>
				</td>
				<td class="borderedCell" colspan="3">
					<p>${renderData.customer}</p>
				</td>
				<td class="borderedCellT">
					<strong>Department</strong>
				</td>
				<td class="borderedCell" colspan="2">
					<p>${renderData.department}</p>
				</td>
			</tr>
			<tr>
				<th class="borderedCellT" >
					<strong>MEMO</strong>
				</th>
				<th class="borderedCell" colspan="3">
					<p>${renderData.memo}</p>
				</th>
				<th class="borderedCellT">
					<strong>Reference No.</strong>
				</th>
				<th class="borderedCell" colspan="2"> 
					<p>${renderData.tranid}</p>
				</th>
			</tr>
			<tr>
				<th class="borderedCellT">
					<strong>Categry</strong>
				</th>
				<th class="borderedCellT" colspan="5">
					<strong>Memo</strong>
				</th>
				<th class="borderedCellT">
					<strong>Amount</strong>
				</th>
			</tr>
			<tr>
				<th class="borderedCellLine" colspan="7" >
					<p>${renderData.line}</p>
				</th>
			</tr>
			<tr>
				<th class="borderedCellT" rowspan="3">
					<strong>BANK INFO</strong>
				</th>
				<th class="borderedCell" rowspan="3" colspan="3">
					<p>${renderData.bankInfo}</p>
				</th>
				<th class="borderedCellT" >
					<strong>Currency</strong>
				</th>
				<th class="borderedCellT" colspan="2">
					<strong>Amount(Total)</strong>
				</th>
			</tr>
			<tr>
				<th class="borderedCell" >
					<p>${renderData.currency}</p>
				</th>
				<th class="borderedCell" colspan="2">
					<p>${renderData.amount}</p>
				</th>
			</tr>
			<tr>
				<th class="borderedCellT" >
					<strong>Due Date</strong>
				</th>
				<th class="borderedCell" colspan="2">
					<p>${renderData.duedate}</p>
				</th>
			</tr>
			<tr>
				<th class="borderedCellT" >
					<strong>GM</strong>
				</th>
				<th class="borderedCellT">
					<strong>VP of Operation</strong>
				</th>
				<th class="borderedCellT">
					<strong>Finance</strong>
				</th>
				<th class="borderedCellT">
					<strong>Accounting</strong>
				</th>
				<th class="borderedCellT">
					<strong>Supervisor</strong>
				</th>
				<th class="borderedCellT">
					<strong>payee</strong>
				</th>
				<th class="borderedCellT">
					<strong>Name</strong>
				</th>
			</tr>
			<tr>
				<th class="borderedCell" >
					<br></br>
					<br></br>
				</th>
				<th class="borderedCell">
					<br></br>
					<br></br>
				</th>
				<th class="borderedCell">
					<br></br>
					<br></br>
				</th>
				<th class="borderedCell">
					<br></br>
					<br></br>
				</th>
				<th class="borderedCell">
					<br></br>
					<br></br>
				</th>
				<th class="borderedCell">
					<br></br>
					<br></br>
				</th>
				<th class="borderedCell">
					<br></br>
					<br></br>
				</th>
			</tr>
		</table>
	</body>
</pdf>